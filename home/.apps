# Nify little check for apps
case $os in
    "Darwin" )
        command -v ack >/dev/null 2>&1 || { echo "I need ack" >&2;};;
    "Debian" )
        command -v ack-grep >/dev/null 2>&1 || { echo "I need ack-grep" >&2; };;
        command -v /usr/bin/gcp >/dev/null 2>&1 || { echo "I need gcp" >&2;}
    "Cent" )
        command -v ack >/dev/null 2>&1 || { echo "I need ack-grep" >&2;};;
esac

command -v zsh >/dev/null 2>&1 || { echo "I need zsh" >&2;}
command -v git >/dev/null 2>&1 || { echo "I need git" >&2;}
command -v homesick >/dev/null 2>&1 || { echo "I need homesick" >&2;}
